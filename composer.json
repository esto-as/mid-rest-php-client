  "type": "library",
  "description": "Mobile-ID Relying Party PHP Api client",
  "license": "MIT",
  "require": {
    "php": "^7.2|^8.0",
    "ext-openssl": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "sop/x509": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
